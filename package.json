{
  "name": "nestjs-graphql-compose",
  "version": "0.1.0",
  "description": "GraphQL Compose module for NestJS",
  "keywords": [
    "nodejs",
    "typescript",
    "nest",
    "nestjs",
    "nest-module",
    "nestjs-module",
    "graphql",
    "compose",
    "graphql-compose",
    "gql",
    "gql-schema",
    "schema",
    "graphql-schema",
    "generation",
    "graphql-generation",
    "graphql-schema-generation"
  ],
  "homepage": "https://github.com/bukhalo/nestjs-graphql-compose#readme",
  "bugs": {
    "url": "https://github.com/bukhalo/nestjs-graphql-compose/issues"
  },
  "license": "MIT",
  "author": "Aleksandr Bukhalo <a@bukhalo.com> (https://github.com/bukhalo)",
  "contributors": [
    "Aleksandr Bukhalo <a@bukhalo.com> (https://github.com/bukhalo)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bukhalo/nestjs-graphql-compose.git"
  },
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "graphql": "^14.1.1",
    "graphql-compose": "^7.14.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-angular": "9.1.2",
    "@nestjs/common": "7.4.2",
    "@nestjs/core": "7.0.5",
    "@types/node": "14.6.0",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.1",
    "graphql": "14.7.0",
    "graphql-compose": "7.19.4",
    "husky": "4.2.3",
    "lint-staged": "10.2.13",
    "prettier": "2.0.2",
    "reflect-metadata": "0.1.13",
    "release-it": "13.7.0",
    "rxjs": "6.6.2",
    "rxjs-compat": "6.5.4",
    "typescript": "3.8.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
}
